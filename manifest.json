{
  "manifest_version": 2,
  
  "name": "Password Generator",
  "description": "Simple extension to generate and save login credentials ",
  "version": "1.0",
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {"16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
  }